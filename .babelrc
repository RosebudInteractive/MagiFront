{
  "presets": ["es2015", "stage-0", "react"], //поддержка ES2015, ES7 и JSX
  "plugins": [
    ['transform-runtime', {
      helpers: false,
      polyfill: false,
      regenerator: true, }],
    "transform-class-properties",
    "transform-es2015-classes",
    "transform-object-assign",
    "es6-promise",
    "transform-decorators-legacy"
  ]
}